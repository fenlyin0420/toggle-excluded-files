{
  "name": "toggle-excluded-files",
  "displayName": "Toggle Excluded Files",
  "publisher": "fenlyin",
  "description": "This extension can be used to toggle hidden files in the explorer.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "first.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "extension.toggleExcludedFiles",
        "title": "Toggle Hidden Excluded Files"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "depenencies": {
    "fenlyin-first": "file:",
    "first": "file:"
  },
  "dependencies": {
    "first": "file:",
    "toggle-excluded-files": "file:",
    "Toggle-Excluded-Files": "file:",
    "toggleExcludedFiles": "file:"
  }
}
