{
  "name": "toggle-excluded-files-fenlyin",
  "displayName": "Toggle Excluded Files Fenlyin",
  "publisher": "fenlyin",
  "description": "This extension can be used to toggle hidden files in the status bar.",
  "icon": "image/icon.png",
  "version": "1.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fenlyin0420/toggle-excluded-files.git"
  },
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.toggleFileState",
        "title": "Toggle File State"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "excludedFilesContainer",
          "title": "Excluded Files",
          "icon": "image/excluded-files-icon.svg"
        }
      ]
    },
    "views": {
      "excludedFilesContainer": [
        {
          "id": "excludedFilesView",
          "name": "Excluded Files"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  }
}
